       nav.style.margin = '0px';
       nav.style.position = 'fixed';
       nav.style.left = '0px';
       nav.style.top = '0px';
       }else{
       nav.style.marginTop = '200px';
       nav.style.position = 'static';
       nav.style.left = '0px';
       nav.style.top = '0px';
       }

}    
