        var str = 'abc987';
        // var reg = new RegExp(/[a-z]/);   // /æ­£åˆ™/ å†™æ³•
        var reg = new RegExp("[a-z]");      // å­—ç¬¦ä¸²å†™æ³•
        console.log( str.match(reg) );
        // è½¬ä¹‰ç¬¦
        var str = 'abc987';
        var reg = new RegExp("[0-9]");  // 
        var reg = new RegExp("\d");     // å­—ç¬¦ä¸²ä¸­çš„\dçš„\ æ˜¯è½¬ä¹‰ç¬¦
        var reg = new RegExp("\\d");    // æ­£ç¡®å†™æ³•
        console.log( str.match(reg) );   
        // è¯†åˆ«å˜é‡
        var str = 'abc987';
        var x = 9;
        var reg = new RegExp("[a-z]x"); // x
        var reg = new RegExp(/[a-z]x/); // x
        var reg = new RegExp("[a-z]"+x);// âˆš
        console.log( str.match(reg) );
        // ä¿®é¥°ç¬¦
        var str = 'abc987';
        var reg = new RegExp("[A-Z]", 'gi'); 
        console.log( str.match(reg));
